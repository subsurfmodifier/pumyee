{
    "manifest_version": 3,
    "name": "Web Page to CSV",
    "version": "1.0",
    "description": "Converts HTML content to CSV",
    "permissions": [
      "activeTab",
      "webNavigation",
      "webRequest",
      "storage"
    ],
    "action": {
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      },
      "default_title": "Convert to CSV",
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  
  
  